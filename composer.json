{
    "name": "innoge/rclone",
    "description": "A sleek PHP wrapper around rclone, exposing its full option set for maximum convenience.",
    "keywords": [
        "InnoGE",
        "rclone"
    ],
    "homepage": "https://github.com/innoge/rclone",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Seuffer",
            "email": "authanram@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "composer/semver": "^3.4",
        "pestphp/pest": "^2.9|^3.0",
        "phpstan/extension-installer": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "InnoGE\\Rclone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InnoGE\\Rclone\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
